 pipeline {
    agent any
    environment{
        ENV_URL = "pipeline.google.com"
        ACCESS_KEY = credentials('AWS_ACCESS_KEY')
        SSH_KEY = credentials('SSH')

    }

    stages {
        stage('First Stage') {
            steps {
                echo 'hello First stage'
                sh 'echo ${ENV_URL}'
                sh "echo access key is .. $ACCESS_KEY"
            }
    }
        stage('Second Stage') {
        environment{
             ENV_URL = "stage.google.com"
            }

            steps {
                echo 'hello second stage'
                sh "echo ${ENV_URL}"
                sh "echo ssh key is .. $SSH_KEY"

            }
    }
        stage('third Stage') {
            steps {
                sh '''
                echo 'Hello Third stage'
                echo 'multiple line printig'
                echo 'ending' '''
            }
    }    
}
}
