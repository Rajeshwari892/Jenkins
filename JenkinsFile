 pipeline {
    agent any
    environment{
        ENV_URL = "pipeline.google.com"
    }

    stages {
        stage('First Stage') {
            steps {
                echo 'First stage'
                sh 'echo ${ENV_URL}'
            }
    }
        stage('Second Stage') {
        environment{
             ENV_URL = "stage.google.com"
            }

            steps {
                echo 'second stage'
                sh 'echo ${ENV_URL}'
            }
    }
        stage('third Stage') {
            steps {
                echo 'Hello World'
            }
    }    
}
}
